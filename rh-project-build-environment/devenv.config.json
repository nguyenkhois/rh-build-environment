{
    "name": "project-devenv",
    "description": "The config file is using for the local front-end development environment",
    "localServer": "test.local",
    "codeBase": [
        "web/app/themes/regionhalland/resources/views/**/*.php"
    ],
    "development": {
        "cssDevPath": "web/_dev/css",
        "scssDevPath": ["web/_dev/scss/*.scss"],
        "jsDevPath": ["web/_dev/js/*.js"]
    },
    "production": {
        "bundleName": "bundle",
        "jsPublicPath": "web/include/scripts",
        "cssPublicPath": "web/include/styles"
    }
}
